<%= turbo_frame_tag dom_id(@object) do %>
  <div class="mx-auto md:w-2/3 w-full">
    <h1 class="font-bold text-4xl">Editing Object</h1>

    <%= render "form", object: @object %>

    <%= link_to "Show this object", @object, class: "ml-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to "Back to objects", json_schema.primitives_objects_path, class: "ml-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
  </div>
<% end %>
