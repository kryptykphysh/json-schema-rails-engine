<% frame_id = dom_id(model, "#{attribute}_field_frame") %>
<%= form_with model: model, class: "contents", data: { turbo_frame: frame_id } do %>
  <%= turbo_frame_tag frame_id do %>
    <%= yield %>

    <%= link_to "Edit", json_schema.edit_polymorphic_path(model), class: "ml-2 rounded-lg py-2 px-3 bg-gray-100 inline text-sm font-normal" %>
  <% end %>
<% end %>
